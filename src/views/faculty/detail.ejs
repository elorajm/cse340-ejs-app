<%- include('../partials/header') %>

<h1><%= facultyMember.name %></h1>

<ul>
  <% if (facultyMember.title) { %>
    <li><strong>Title:</strong> <%= facultyMember.title %></li>
  <% } %>

  <li><strong>Department:</strong> <%= facultyMember.department %></li>

  <% if (facultyMember.office) { %>
    <li><strong>Office:</strong> <%= facultyMember.office %></li>
  <% } %>

  <% if (facultyMember.phone) { %>
    <li><strong>Phone:</strong> <%= facultyMember.phone %></li>
  <% } %>

  <% if (facultyMember.email) { %>
    <li><strong>Email:</strong> <%= facultyMember.email %></li>
  <% } %>
</ul>

<p>
